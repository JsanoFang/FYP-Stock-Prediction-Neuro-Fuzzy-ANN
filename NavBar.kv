<MyLayout>
    BoxLayout:
        orientation: 'vertical'
        size: root.width, root.height
        size_hint: None, None


        MDBottomNavigation:
            size: root.width,root.height
            anchor_x:'right'
            anchor_y:'bottom'
            MDBottomNavigationItem:

                name: 'home'
                text: 'Home'
                icon: 'home'
                on_enter:root.update_home()
                MDToolbar:
                    size_hint:(None,None)
                    valign:'top'
                    size: root.width,50
                    anchor_y:'top'
                    anchor_x:'right'
                    title: "Dashboard"
                    font_size:32

                GridLayout:
                    row_default_height: '30dp'

                    cols:4
                    rows:10
                    Label:
                        text: "Company Name"
                        font_size: 17
                        color:0,1,1,1
                    Label:
                        text:"Today's Stock Price (USD)"
                        front_size:25
                        color:0,1,1,1
                    Label:
                        text:"Profit/Loss (USD)"
                        font_size:17
                        color:0,1,1,1

                    Label:
                        text:"Bookmarked ?"
                        front_size:25
                        color:0,1,1,1

                    Label:
                        text:"Tesla"

                    Label:
                        id:home_prc_tesla
                        text: ""

                    Label:
                        id:home_prof_tesla
                        text: ""

                    Label:
                        id:home_book_tesla
                        text: ""

                    Label:
                        text: "Sony"

                    Label:
                        id:home_prc_sony
                        text: ""

                    Label:
                        id:home_prof_sony
                        text: ""

                    Label:
                        id:home_book_sony
                        text: ""
                    Label:
                        text: "LG"

                    Label:
                        id:home_prc_lg
                        text: ""

                    Label:
                        id:home_prof_lg
                        text: ""

                    Label:
                        id:home_book_lg
                        text: ""
                    Label:
                        text: "Hyundai"

                    Label:
                        id:home_prc_hyundai
                        text: ""

                    Label:
                        id:home_prof_hyundai
                        text: ""

                    Label:
                        id:home_book_hyundai
                        text: ""
                    Label:
                        text: "Google"

                    Label:
                        id:home_prc_google
                        text: ""

                    Label:
                        id:home_prof_google
                        text: ""

                    Label:
                        id:home_book_google
                        text: ""
                    Label:
                        text: "Easterly Government Properties"
                        font_size:13
                    Label:
                        id:home_prc_dea
                        text: ""

                    Label:
                        id:home_prof_dea
                        text: ""

                    Label:
                        id:home_book_dea
                        text: ""

                    Label:
                        text: "Nintendo"

                    Label:
                        id:home_prc_nintendo
                        text: ""

                    Label:
                        id:home_prof_nintendo
                        text: ""

                    Label:
                        id:home_book_nintendo
                        text: ""




            MDBottomNavigationItem:
                name: 'tesla'
                text: 'Tesla'
                icon: 'rocket-launch-outline'
                on_enter:root.add_tesla_history()
                MDToolbar:
                    size_hint:(None,None)
                    valign:'top'
                    size: root.width,50
                    anchor_y:'top'
                    anchor_x:'right'
                    title: "Tesla's Stock Prediction"
                    font_size:32

                GridLayout:
                    row_default_height: '30dp'

                    cols:2
                    rows:15
                    Label:
                        text: "Bookmark"
                        font_size: 20
                    CheckBox:
                        id:box_tesla
                        on_active: root.checkbox_click(self,self.active,"TSLA")
                    Button:
                        id:view_trained_model_tesla
                        text:"View Trained Model"
                        height: 30
                        width: 250
                        on_press: root.show_trained("tesla")

                    Button:
                        id:view_stock_tesla
                        text:"View Today's Stock Data"
                        height: 30
                        width: 250
                        on_press: root.show_stock_now("TSLA")


                    Label:
                        text:"Input Amount of Stock Purchased"

                    TextInput:
                        id:stock_amt_tesla
                        size_hint: (None, None)
                        height: 30
                        width: 350
                        multiline: False
                    Label:
                        text:"Input Price of stock when Purchased"

                    TextInput:
                        id:stock_buy_prc_tesla
                        size_hint: (None, None)
                        height: 30
                        width: 350
                        multiline: False
                    Label:
                    Button:
                        id:save_stock_buy_tesla
                        text:"Save Information"
                        size_hint: (None, None)
                        height: 30
                        width: 250
                        on_press: root.update_buy(1,stock_amt_tesla.text,stock_buy_prc_tesla.text)

                    Label:
                        text:"Tesla's Data Needed for Prediction Model"
                        front_size:20

                    Label:
                        size_hint: (None, None)
                        width:0
                        height:0

                    Label:
                        text: "Input High Value of Stock"
                        font_size: 20

                    TextInput:
                        id:high_tesla
                        size_hint: (None, None)
                        height: 30
                        width: 350
                        multiline: False

                    Label:
                        text: "Input Low Value of Stock"
                        font_size: 20

                    TextInput:
                        id:low_tesla
                        size_hint: (None, None)
                        height: 30
                        width: 350
                        multiline: False

                    Label:
                        text: "Input Open Value of Stock"
                        font_size: 20

                    TextInput:
                        id:open_tesla
                        size_hint: (None, None)
                        height: 30
                        width: 350
                        multiline: False

                    Label:
                        text: "Input Volume Value of Stock"
                        font_size: 20

                    TextInput:
                        id:vol_tesla
                        size_hint: (None, None)
                        height: 30
                        width: 350
                        multiline: False

                    Label:
                        text: "Input Adjusted Close Value of Stock"
                        font_size: 20

                    TextInput:
                        id:adjcls_tesla
                        size_hint: (None, None)
                        height: 30
                        width: 350
                        multiline: False

                    Label:
                        id:result_tesla
                        font_size:20
                    Button:
                        id:predict_tesla
                        text:"Predict"
                        size_hint: (None, None)
                        height: 30
                        width: 250
                        on_press:root.button_predict_stock(low_tesla.text,high_tesla.text,open_tesla.text,vol_tesla.text,adjcls_tesla.text,"tesla","TSLA")
                    Label:
                        size_hint: (None, None)
                        width: 300
                        height: 1
